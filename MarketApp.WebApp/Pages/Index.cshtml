@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <table class="table">
    <thead>
        <tr>
            
            <td>Product Name</td>
            <td>dis</td>
            <td>SKUBarkod</td>
            <td>Product Name</td>
            <td>dis</td>
            <td>SKUBarkod</td>
            <td>Product Name</td>
            <td>dis</td>
            <td>SKUBarkod</td>
            <td>Product Name</td>
            <td>dis</td>

        </tr>
    </thead>

    <tbody>
        @foreach(var item in Model.ProductsWeb)
        {
            
            <tr>
                <td>@item.SKU</td>
                <td>@item.ProductName</td>
                <td>@item.ProductDescirption</td>
                <td>@item.CategoryID</td>
                <td>@item.SupplierID</td>
                <td>@item.TaxId</td>
                <td>@item.MSRP</td>
                <td>@item.UnitPrice</td>
                <td>@item.UnitsInStock</td>
                <td>@item.QuantityPerUnit</td>
                <td>@item.Discontinued</td>
            </tr>
        }
    </tbody>
</table>
</div>
